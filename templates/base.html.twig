<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	{% block body %}
		<body>
			<header class="sticky top-0 z-40">
				{% block header %}
					{% include "header.html.twig" %}
				{% endblock %}
			</header>
			<main>
				<div class="sm:p-6 max-w-screen-xl mx-auto">
					{% block main %}
						{#  #}
					{% endblock %}
				</div>
			</main>
			<footer>
				{% block footer %}{% endblock %}
			</footer>
		</body>
	{% endblock %}
	{% block javascript %}
	{% endblock %}
</html>
